	function selectedColumnNames(checkedColumns,totalColumns,names){
		console.log("names construction "+checkedColumns+" "+totalColumns+" "+names);
		if(checkedColumns ==0){
			$('.'+buttonParentDivName+' div.ms-options-wrap > button').html("Select Columns");
		}else if(checkedColumns< totalColumns){
			$('.'+selectAll_class).prop("checked",false);
			$('.'+buttonParentDivName+' div.ms-options-wrap > button').html(names);
		}else if(checkedColumns==totalColumns){
			$('.'+selectAll_class).prop("checked",true);
			$('.'+buttonParentDivName+' div.ms-options-wrap > button').html("All");
		}
